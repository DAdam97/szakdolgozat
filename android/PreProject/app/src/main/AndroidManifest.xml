    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PreProject">
        <activity android:name=".AirActivity"></activity>
        <activity android:name=".WarningActivity" />
        <activity android:name=".SecurityActivity" />
        <activity android:name=".DoorsActivity" />
        <activity android:name=".LightActivityChildrensRoom" />
        <activity android:name=".LightActivityBathRoom" />
        <activity android:name=".LightActivityKitchen" />
        <activity android:name=".LightActivityGarden" />
        <activity android:name=".LightActivityLivingRoom" />
        <activity android:name=".LightActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>